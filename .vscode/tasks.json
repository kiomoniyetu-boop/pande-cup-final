{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run React tests",
			"type": "shell",
			"command": "npm",
			"args": [
				"test"
			],
			"isBackground": false,
			"problemMatcher": [
				"$jest-stare"
			],
			"group": "test"
		},
		{
			"label": "Start React app",
			"type": "shell",
			"command": "npm",
			"args": [
				"start"
			],
			"isBackground": false,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		}
	]
}